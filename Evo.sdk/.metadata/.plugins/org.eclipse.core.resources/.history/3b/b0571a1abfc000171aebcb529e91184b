#include "parser.h"

u32 parser(u8 *cmd, u8 *result)
{
	u32 status = -1;

	if (strcmp(cmd, "RN00000000000000") == 0) {
		status = 0;
		strcpy(result, "OK00000000000000");
	} else
		strcpy(result, "ER00000000000000");

	return status;
}
