#include "parser.h"

u32 parser(u8 *cmd, u8 *result)
{
	u32 status = -1;
	u32 cmd_value = 0;

	cmd_value = atoi((char *)cmd);

	if ((cmd_value & TYPE_MASK) == 0x00000000) {
		status = 0;
		strcpy(result, "OK00000000000000");
	}
	if ((cmd[0] == 'O') && (cmd[1] == 'K')) {
		status = 0;
		strcpy(result, "OK00000000000000");
	} else if ((cmd[0] == 'R') && (cmd[1] == 'N')) {
		status = 1;
		strcpy(result, "CR00000000000000");
	} else
		strcpy(result, "ER00000000000000");

	return status;
}
